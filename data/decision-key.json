  "steps": {
    "1": {
      "question": "Does the call have five or more pulses?",
      "sonogram": "1.svg",
      "type": "yes-no",
      "options": {
        "no": "STOP",
        "yes": "2"
      }
    },
    "2": {
      "question": "Are at least 5 to 10 pulses within the call sequence distributed fairly evenly?",
      "sonogram": "2.svg",
      "type": "yes-no",
      "options": {
        "no": "STOP",
        "yes": "3"
      }
    },
    "3": {
      "question": "Are these pulses distributed about 0.1 seconds or more apart?",
      "sonogram": "3.svg",
      "type": "yes-no",
      "options": {
        "no": "STOP",
        "yes": "4"
      }
    },
    "4": {
      "question": "Are the individual pulses of good quality?",
      "sonogram": "4.svg",
      "type": "yes-no",
      "options": {
        "no": "STOP",
        "yes": "5"
      }
    },
    "5": {
