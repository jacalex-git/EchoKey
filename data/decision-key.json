{
  "version": "2.0",
  "title": "EchoKey Acoustic Identification Guide",
  "lastUpdated": "2026-03",
  "description": "ECHOKEY Decision Key v2. For use with calls pre-screened by Kaleidoscope Pro or SonoBat. Review file in True Time mode before beginning.",
  "species": {
    "LABO": {
      "code": "LABO",
      "commonName": "Eastern Red / Seminole Bat",
      "scientificName": "Lasiurus borealis / Lasiurus seminolus",
      "sonogram": "LABO.svg"
    },
    "NYHU": {
      "code": "NYHU",
      "commonName": "Evening Bat",
      "scientificName": "Nycticeius humeralis",
      "sonogram": "NYHU.svg"
    },
    "PESU": {
      "code": "PESU",
      "commonName": "Tricolored Bat",
      "scientificName": "Perimyotis subflavus",
      "sonogram": "PESU.svg"
    },
    "MYAU": {
      "code": "MYAU",
      "commonName": "Southeastern Bat",
      "scientificName": "Myotis austroriparius",
      "sonogram": "MYAU.svg"
    },
    "MYLE": {
      "code": "MYLE",
      "commonName": "Eastern Small-footed Bat",
      "scientificName": "Myotis leibii",
      "sonogram": "MYLE.svg"
    },
    "MYGR": {
      "code": "MYGR",
      "commonName": "Gray Bat",
      "scientificName": "Myotis grisescens",
      "sonogram": "MYGR.svg"
    },
    "MYSE": {
      "code": "MYSE",
      "commonName": "Northern Long-eared Bat",
      "scientificName": "Myotis septentrionalis",
      "sonogram": "MYSE.svg"
    },
    "MYSO": {
      "code": "MYSO",
      "commonName": "Indiana Bat",
      "scientificName": "Myotis sodalis",
      "sonogram": "MYSO.svg"
    },
    "MYLU": {
      "code": "MYLU",
      "commonName": "Little Brown Bat",
      "scientificName": "Myotis lucifugus",
      "sonogram": "MYLU.svg"
    }
  },
  "results": {
    "STOP": { "type": "stop", "message": "Call quality insufficient for species identification" },
    "NON_TARGET": { "type": "non-target", "message": "Non-target species" },
    "UNKNOWN": { "type": "unknown", "message": "Unknown Bat" },
    "UNIDENTIFIED_40K": { "type": "group", "message": "Unidentified 40 kHz Myotis" },
    "SOCIAL_CALL": { "type": "social", "message": "Social Call" },
    "MYAU_MYLE": { "type": "group", "message": "MYAU or MYLE (range overlap)" }
  },
  "steps": {

    "1": {
      "question": "Does the call sequence have five or more pulses?",
      "sonogram": "1.svg",
      "type": "yes-no",
      "options": { "no": "STOP", "yes": "2" }
    },

    "2": {
      "question": "Are at least 5 to 10 pulses distributed fairly evenly?",
      "sonogram": "2.svg",
      "type": "yes-no",
      "options": { "no": "STOP", "yes": "3" }
    },

    "3": {
      "question": "Are there generally fewer than 12 pulses per second (pulses should not accelerate into a feeding buzz)?",
      "sonogram": "3.svg",
      "type": "yes-no",
      "options": { "no": "STOP", "yes": "4" }
    },

    "4": {
      "question": "Are the individual pulses of good quality?",
      "sonogram": "4.svg",
      "type": "yes-no",
      "options": { "no": "STOP", "yes": "4b" }
    },

    "4b": {
      "question": "Are harmonics visible in the recording?",
      "sonogram": "4b.svg",
      "type": "yes-no",
      "note": "Files without harmonics proceed with reduced confidence.",
      "options": { "no": "5", "yes": "5" }
    },

    "5": {
      "question": "Is the call from only one bat?",
      "sonogram": "5.svg",
      "type": "yes-no",
      "options": { "no": "STOP", "yes": "6" }
    },

    "6": {
      "question": "Is the call very obviously bouncy and inconsistent?",
      "sonogram": "6.svg",
      "type": "yes-no",
      "options": { "yes": "6b", "no": "7" }
    },

    "6b": {
      "question": "Is the Fmin of the bouncy call above 35 kHz?",
      "sonogram": "6b.svg",
      "type": "multiple-choice",
      "options": {
        "Yes": "LABO",
        "No, accept non-target": "NON_TARGET",
        "No, continue manual review": "6c"
      }
    },

    "6c": {
      "question": "This portion of the key is under construction.",
      "type": "info",
      "next": "7"
    },

    "7": {
      "question": "Are search phase pulses reverse J-shaped without strong tails?",
      "sonogram": "7.svg",
      "type": "yes-no",
      "options": { "yes": "9", "no": "20" }
    },

    "9": {
      "question": "Does Fmin stay above 38 kHz?",
      "sonogram": "9.svg",
      "type": "yes-no",
      "options": { "yes": "10", "no": "9b" }
    },
     "9b": {
      "question": "This call sequence Fmin is too low for it to be a target species. Would you like to continue?",
      "type": "multiple-choice",
      "options": {
        "Yes, continue manual review": "6c",
        "No, accept non-target": "NON_TARGET"
    },
    "10": {
      "question": "Is Fmin steady across most pulses?",
      "sonogram": "10.svg",
      "type": "yes-no",
      "options": { "yes": "11", "no": "NYHU" }
    },

    "11": {
      "question": "Are the calls distinctly hockey-stick shaped?",
      "sonogram": "11.svg",
      "type": "yes-no",
      "options": { "yes": "PESU", "no": "NYHU" }
    },

    "20": {
      "question": "Are pulses sigmoidal with distinct tails?",
      "sonogram": "20.svg",
      "type": "yes-no",
      "options": { "yes": "21", "no": "40" }
    },

    "21": {
      "question": "Do pulses have steady Fc values with strong tails?",
      "sonogram": "21.svg",
      "type": "yes-no",
      "options": { "no": "22", "yes": "23" }
    },

    "22": {
      "question": "Only high quality Myotis calls should proceed. Continue?",
      "sonogram": "22.svg",
      "type": "yes-no",
      "options": { "no": "UNIDENTIFIED_40K", "yes": "23" }
    },

    "23": {
      "question": "What is the average Fc value?",
      "sonogram": "23.svg",
      "type": "multiple-choice",
      "options": {
        "Less than 42 kHz": "25",
        "Greater than 42 kHz": "30"
      }
    },

    "25": {
      "question": "How long are search phase pulses?",
      "sonogram": "25.svg",
      "type": "multiple-choice",
      "options": {
        "Greater than 7 ms": "60",
        "5–6 ms": "26",
        "Less than 5 ms": "31"
      }
    },

    "26": {
      "question": "Is there a strong basket shape before the tail?",
      "sonogram": "26.svg",
      "type": "yes-no",
      "options": { "yes": "33", "no": "60" }
    },

    "30": {
      "question": "Are pulses strongly inflected (knee/heel/toe) around 45–52 kHz?",
      "sonogram": "30.svg",
      "type": "multiple-choice",
      "options": {
        "Inflected": "MYGR",
        "Mostly smooth": "31"
      }
    },

    "31": {
      "question": "Is there a strong basket shape before the tail?",
      "sonogram": "26.svg",
      "type": "yes-no",
      "options": { "yes": "33", "no": "32" }
    },

    "32": {
      "question": "Is the call very high bandwidth (>75 kHz)?",
      "type": "yes-no",
      "options": { "yes": "60", "no": "34" }
    },

    "33": {
      "question": "Was the call collected in known range of MYAU, MYLE, or both?",
      "sonogram": "MYAU_MYLE.svg",
      "type": "multiple-choice",
      "options": {
        "MYAU range only": "MYAU",
        "MYLE range only": "MYLE",
        "Both overlap": "MYAU_MYLE"
      }
    },

    "34": {
      "question": "Could this be Eastern Red / Seminole Bat (steep variant)?",
      "sonogram": "LABO.svg",
      "type": "multiple-choice",
      "options": {
        "Yes, bouncing and variable timing": "LABO",
        "No, steady timing (~100 ms)": "60"
      }
    },

    "40": {
      "question": "Are pulses very flat with low slope?",
      "sonogram": "40.svg",
      "type": "yes-no",
      "options": { "yes": "41", "no": "50" }
    },

    "41": {
      "question": "Is Fmin greater than 38 kHz?",
      "type": "multiple-choice",
      "options": {
        "Yes": "PESU",
        "No, accept non-target": "NON_TARGET",
        "No, continue manual review": "6c"
      }
    },

    "50": {
      "question": "Are pulses very steep straight lines?",
      "type": "yes-no",
      "options": { "yes": "51", "no": "70" }
    },

    "51": {
      "question": "Are pulses very bouncy and inconsistent?",
      "type": "multiple-choice",
      "options": {
        "Yes": "LABO",
        "No": "52"
      }
    },

    "52": {
      "question": "Are pulses very high bandwidth (>75 kHz)?",
      "type": "yes-no",
      "options": { "yes": "60", "no": "34" }
    },

    "60": {
      "question": "In AnalookW, what is the minimum slope value?",
      "type": "multiple-choice",
      "options": {
        "Slope > 200": "MYSE",
        "100 < Slope < 200": "MYSO",
        "Slope < 100": "MYLU"
      }
    },

    "70": {
      "question": "Is the call composed of varied pulse shapes and frequencies?",
      "type": "yes-no",
      "options": { "yes": "SOCIAL_CALL", "no": "UNKNOWN" }
    }

  },

  "startStep": "1"
}
